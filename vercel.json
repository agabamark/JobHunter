{
  "version": 2,
  "functions": {
    "api/signup.py": {
      "memory": 1024,
      "maxDuration": 10
    },
    "api/status.py": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/favicon.ico",
      "dest": "/public/favicon.ico"
    },
    {
      "src": "/api/signup",
      "methods": ["POST", "OPTIONS"],
      "dest": "/api/signup.py"
    },
    {
      "src": "/api/status",
      "methods": ["GET", "OPTIONS"],
      "dest": "/api/status.py"
    }
  ]
}